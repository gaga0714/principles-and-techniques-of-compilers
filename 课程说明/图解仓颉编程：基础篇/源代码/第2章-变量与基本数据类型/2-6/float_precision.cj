from std import format.*

main() {
    let x: Float16 = 0.1234567890123456789
    let y: Float32 = 0.1234567890123456789
    let z: Float64 = 0.1234567890123456789

    println(x.format(".18"))  // 输出：0.123474121093750000
    println(y.format(".18"))  // 输出：0.123456791043281555
    println(z.format(".18"))  // 输出：0.123456789012345677
}
