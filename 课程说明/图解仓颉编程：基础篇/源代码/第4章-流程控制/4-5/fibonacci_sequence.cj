main() {
    var a = 1  // 当前项的前面第2项，初始值为数列第1项的值
    var b = 1  // 当前项的前一项，初始值为数列第2项的值
    var c = 0  // 当前项，通过前2项之和计算得到
    var i = 3  // 计数器，用于计项数
    print("第1项：${a}\t第2项：${b}\t")  // 输出第1项和第2项

    do {
        c = a + b  // 计算当前项
        a = b
        b = c
        print("第${i}项：${c}\t")
        if (i % 5 == 0) {
            print("\n")
        }
        i++
    } while (i < 11)
}
